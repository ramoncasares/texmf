  \ifx\xmpJournaltitle\empty\else<prism:aggregationType><rdf:Bag><rdf:li>journal</rdf:li></rdf:Bag></prism:aggregationType>\fi
  \ifx\xmpJournaltitle\empty\else<prism:publicationName>\xmpJournaltitle</prism:publicationName>\fi
  \ifx\xmpJournalnumber\empty\else<prism:issn>\xmpJournalnumber</prism:issn>\fi
  \ifx\xmpVolume\empty\else<prism:volume>\xmpVolume</prism:volume>\fi
